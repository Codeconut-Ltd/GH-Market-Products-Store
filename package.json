{
  "name": "market-products-store",
  "version": "0.0.1",
  "private": true,
  "homepage": "/kassensystem/",
  "scripts": {
    "build": "set NODE_ENV=production && npx react-scripts build --env=production",
    "build:dev": "npx react-scripts build",
    "start": "npx react-scripts start",
    "run": "npx serve build",
    "lint:style:check": "npx stylelint \"src/**/*.scss\"",
    "lint:style:fix": "npx stylelint \"src/**/*.scss\" --fix",
    "lint:script:eslint:check": "npx eslint",
    "lint:script:eslint:fix": "npx eslint --fix \"src/**/*.ts*\"",
    "lint:script:prettier:check": "npx prettier --check \"src/**/*.(scss|ts*)\"",
    "lint:script:prettier:fix": "npx prettier --write \"src/**/*.(scss|ts*)\"",
    "test": "npx react-scripts test",
    "test:debug": "npx react-scripts --inspect-brk test --runInBand --no-cache",
    "project:clean": "rm -rf node_modules"
  },
  "dependencies": {
    "axios": "0.24.0",
    "bootstrap": "5.1.3",
    "dompurify": "2.3.4",
    "i18next": "21.6.5",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-bootstrap": "2.1.0",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.2.2",
    "react-i18next": "11.15.3",
    "react-icons": "4.3.1",
    "react-router-dom": "6.2.1",
    "react-scripts": "5.0.0",
    "recoil": "0.5.2",
    "styled-components": "5.3.3",
    "web-vitals": "2.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.0",
    "@types/node": "16.11.19",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-sonarjs": "0.11.0",
    "hint": "6.1.9",
    "htmlhint": "1.1.3",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "react-styleguidist": "11.2.0",
    "sass": "1.47.0",
    "serve": "13.0.2",
    "snyk": "1.1064.0",
    "stylelint": "14.2.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "24.0.0",
    "stylelint-config-standard-scss": "3.0.0",
    "typescript": "4.5.4"
  }
}
